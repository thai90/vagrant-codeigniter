# ! /bin/bash -x
function accessCount
{
    accessNum=0
    if [ -f "$1" ]; then
        while read line
        do
            if [[ "$line" == *"$2"* ]]; then
                ((accessNum++))
            fi
        done <$1
        echo $accessNum
    else
        echo "File not found"
    fi
}

logFilePath="/var/log/httpd/vagrant-codeigniter-access_log"
loginPageUrl="/user/login HTTP/1.1"
regPageUrl="/user/register HTTP/1.1"
homePageUrl="/user/homepage HTTP/1.1"

echo $(accessCount $logFilePath $loginPageUrl)
echo $(accessCount $logFilePath $regPageUrl)
echo $(accessCount $logFilePath $homePageUrl)